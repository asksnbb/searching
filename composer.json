{
    "name": "monogram/searching",
    "description": "",
    "keywords": ["laravel","search","fuzzy"],
    "license": "MIT",
    "authors": [
        {
            "name": "monogram", 
            "email": "asksnbb@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.4 || ^4.0 || ^5.0",
        "phpunit/phpunit": "^4.8 || ^5.7 || ^6.3 || ^7.0"
    },
    "autoload": {
        "psr-4": {
            "Monogram\\Searching\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Monogram\\Tests\\Searching\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
    "laravel": {
        "providers": [
            "Monogram\\Searching\\src\\SearchingServiceProvider"
        ],
        "aliases": {
            "Monogram": "Monogram\\Monogram\\Facades\\Monogram"
        }
    }
},

    "minimum-stability": "dev",
    "prefer-stable": true
}
